<!DOCTYPE html>
<html>

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width" />
  <title>
    <%= context.title %>
  </title>
  <% if (context.rss) { %>
    <link rel="alternate" type="application/atom+xml" href="<%= context.rss.href %>" title="<%= context.rss.title %>">
    <% } %>

      <% for (var file of context.cssFiles) { %>
        <link href="<%= file %>" rel="stylesheet">
        <% } %>
</head>

<body>
  <%- context.html %>

    <% for (var script of context.jsFiles) { %>
      <script src="<%= script %>" type="text/javascript"></script>
      <% } %>
</body>

</html>
